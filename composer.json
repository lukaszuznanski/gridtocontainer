{
    "name": "sbublies/gridtocontainer",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "description": "EXT:gridtocontainer is a small migration extension with backend module for those who want to switch from EXT:gridelements to EXT:container.",
    "authors": [
        {
            "name": "Stefan Bublies",
            "role": "Developer"
        },
        {
            "name": "Daniel PiÄ™tka",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^11.5"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "SBublies\\Gridtocontainer\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "gridtocontainer"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
